{
  "name": "catalyst",
  "version": "1.0.0",
  "description": "JavaScript development environment starter kit",
  "scripts": {
    "start": "npm-run-all --parallel gen-css-types open:src start-mockapi",
    "open:src": "babel-node bld/srcServer",
    "lint": "tslint --project tsconfig.json --config tslint.yaml --format codeFrame src/**/* bld/**/*",
    "lint:fix": "tslint --project tsconfig.json --config tslint.yaml --fix src/**/* bld/**/*",
    "lint:vscode": "tslint --project tsconfig.json --config tslint.yaml src/**/* bld/**/* -t verbose",
    "lint:watch": "npm run watch -- --command \"npm run lint\"",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain posten",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "jest",
    "test:watch": "jest --watch --verbose false",
    "gen-mock-data": "babel-node bld/json-server/generateMockData",
    "gen-css-types": "tcm -w -p src/**/*.scss",
    "prestart-mockapi": "npm run gen-mock-data",
    "start-mockapi": "json-server bld/json-server/db.json --config bld/json-server/config.json",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node bld/build.js",
    "postbuild": "babel-node bld/distServer.js",
    "check-types": "tsc",
    "watch": "chokidar \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" --ignore \"node_modules\" --silent --initial"
  },
  "author": "Zach Posten",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "@types/classnames": "^2.2.3",
    "@types/memoize-one": "^4.1.0",
    "@types/node": "^9.4.6",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.38",
    "@types/react-bootstrap": "^0.32.6",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-bootstrap": "^0.24.4",
    "@types/react-router-dom": "^4.2.4",
    "@types/recompose": "^0.30.2",
    "@types/redux-immutable-state-invariant": "^2.0.4",
    "@types/toastr": "^2.1.35",
    "@types/webpack-env": "^1.13.5",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.1",
    "chance": "^1.0.16",
    "classnames": "^2.2.5",
    "compression": "1.6.2",
    "css-loader": "0.25.0",
    "faker": "^4.1.0",
    "history": "^4.7.2",
    "ifdef-loader": "^2.0.3",
    "memoize-one": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.13.1",
    "notistack": "^0.4.1",
    "npm-run-all": "3.1.1",
    "postcss-loader": "^2.1.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.13.1",
    "typescript": "^2.7.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.20.2",
    "webpack-md5-hash": "0.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^23.3.10",
    "babel-jest": "^23.6.0",
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "4.14.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-serializer-html": "^5.0.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.5.0-rc16",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "react-hot-loader": "^4.3.11",
    "rimraf": "^2.5.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.5.1",
    "typed-css-modules": "^0.3.6",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
